{
  "name": "weatherapp",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "lint": "expo lint",
    "prettier": "prettier --config .prettierrc.json --check \"{app,components,utils,hooks,models,store}/**/*.{ts,tsx}\"",
    "prettier-fix": "prettier --config .prettierrc.json --write \"{app,components,utils,hooks,models,store}/**/*.{ts,tsx}\""
  },
  "jest": {
    "preset": "jest-expo",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "components/**/*.{js,jsx,ts,tsx}",
      "!**/node_modules/**",
      "!**/babel.config.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@reduxjs/toolkit": "^2.2.4",
    "@testing-library/react-native": "^12.5.0",
    "eslint-plugin-testing-library": "^6.2.2",
    "expo": "~51.0.4",
    "expo-constants": "~16.0.1",
    "expo-font": "~12.0.5",
    "expo-linear-gradient": "~13.0.2",
    "expo-linking": "~6.3.1",
    "expo-location": "~17.0.1",
    "expo-router": "~3.5.12",
    "expo-splash-screen": "~0.27.4",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.4",
    "expo-web-browser": "~13.0.3",
    "i18n-iso-countries": "^7.11.1",
    "lodash": "^4.17.21",
    "metro-react-native-babel-preset": "^0.77.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.1",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-web": "~0.19.10",
    "react-redux": "^9.1.2",
    "redux-mock-store": "^1.5.4",
    "redux-observable": "^2.0.0",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^3.1.0",
    "rxjs": "^7.8.1",
    "ts-jest": "^29.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/plugin-transform-react-jsx": "^7.23.4",
    "@babel/preset-env": "^7.24.5",
    "@testing-library/jest-native": "^5.4.3",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.1",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "@types/redux-mock-store": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "babel-jest": "^29.7.0",
    "babel-preset-expo": "^11.0.5",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.1",
    "prettier": "^3.2.5",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "private": true
}
